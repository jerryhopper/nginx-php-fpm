{
  "name": "adriansuter/slim4-skeleton",
  "description": "Web application skeleton that uses the Slim4 Framework, PHP-DI, Nyholm PSR7 and Twig.",
  "license": "MIT",
  "authors": [
    {
      "name": "Adrian Suter-Wirz",
      "email": "adrian@suter-wirz.ch"
    }
  ],
  "autoload": {
    "psr-4": {
      "App\\": "application/App/"
    }
  },
  "require": {
    "cloudflare/sdk": "^1.1",
    "php": "^7.2",
    "slim/slim": "^4.1",
    "slim/twig-view": "^3.1",
    "nyholm/psr7": "^1.2",
    "nyholm/psr7-server": "^0.4",
    "php-di/php-di": "^6",
    "monolog/monolog": "^2.0",
    "jerryhopper/service-discovery-php": "^0.1.11",
    "symfony/http-foundation": "^5.2",
    "acmephp/core": "^1.2",
    "league/oauth2-client": "^2.6",
    "jerryhopper/oauth2-fusionauth": "^0.0.2",
    "akrabat/ip-address-middleware": "^2.0",
    "illuminate/database": "^7.30"
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5",
    "phpspec/prophecy": "^1.8"
  },
  "scripts": {
    "start": "php -S localhost:8080 -t public",
    "test": [
      "@phpunit"
    ],
    "phpunit": "php vendor/bin/phpunit"
  }
}
